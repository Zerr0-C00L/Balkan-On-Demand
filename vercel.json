{
  "version": 2,
  "installCommand": "npm install && cd configure && npm install && npm run build",
  "builds": [
    {
      "src": "addon.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/configure/(.*)",
      "dest": "/addon.js"
    },
    {
      "src": "/(.*)",
      "dest": "/addon.js"
    }
  ],
  "public": true
}
